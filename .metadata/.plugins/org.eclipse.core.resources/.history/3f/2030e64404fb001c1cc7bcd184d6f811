package com.xworkz.array.jaggedarray;

public class JaggedArray {
public static void main(String[] args) {
		        int arr[][] = new int[3][];  
		        arr[0] = new int[3];  
		        arr[1] = new int[4];  
		        arr[2] = new int[2];  
		       int count = 6;  
		        for (int i=0; i<arr.length; i++)  
		            for(int j=1; j<arr[i].length; j++)  
		                arr[i][j] =count;
		                count++; 
		        for (int i=1; i<arr.length; i++){  
		            for (int j=1; j<arr[i].length; j++){  
		                System.out.print(arr[i][j]+" ");  
		            }  
		            System.out.println();  
		        }  
		    }  
		  }


