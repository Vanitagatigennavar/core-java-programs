package com.xworkz_collectionTest_arraylist;

import java.util.ArrayList;
//import java.util.Iterator;

public class ArraylistDemoWithTypesafe {

	public static void main(String[] args) {
       Book book=new Book("classmate",100,40);
       book.getBrand();
       book.getNoofpages();
       book.getPrice();
       
       Book book1=new Book("suleka", 200, 50);
       book1.getBrand();
       book1.getNoofpages();
       book1.getPrice();
       
       ArrayList <Book> al=new ArrayList<Book>();
       al.add(book);
       al.add(book1);
       
//       for (Iterator iterator = al.iterator(); iterator.hasNext();) {
//		Book book2 = (Book) iterator.next();
       for (int i = 0; i <al.size(); i++) {
		if(al.get(i).getNoofpages() == 200) {
			System.out.println(al.get(i));
			
	}
//	    System.out.println(book);
//	    System.out.println(book1);
	}
	}

}
