package com.xworkz.BasicPrograms;

public class pattern2 {

	public static void main(String[] args) {
		
		
        for(int i=1;i<=2;i++)
{
	    int num=1;
      for(int l=1;l<=4;l++)
        {
	
           num++;
           System.out.print(num);
            num++;
        }
           System.out.println();
}
}

	}


