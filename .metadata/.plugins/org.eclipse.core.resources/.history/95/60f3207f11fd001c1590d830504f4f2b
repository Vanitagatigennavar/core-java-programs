package com.xworkz_String;

public class StringDemo 
{
    public static void main(String[] args) 
	{
       String name="shankar";
       String name1="shankar";
       System.out.println(name==name1);
       
       String vanita=new String ("vanita");
       String internVanita=vanita.intern();
       System.out.println(name==name1);
       System.out.println(vanita==internVanita);
       String vani="vanita";
       System.out.println(internVanita==vani);
       name1="bhoomika";
       System.out.println(name==name1);
       String vanita1=new String("vanita");
       System.out.println(vanita==vanita1);
       }
}
