package com.xworkz.object_class_method.clone_method.soap;

public class Soap {
	String type;
	String color;
	double price;

	Soap(String tyfe, String color, double price) {
		this.type = type;
		this.color = color;
		this.price = price;
	}

	public void copy() throws CloneNotSupportedException {
		Object obj = super.clone();
		Soap soap = (Soap) obj;
		return;
	}		
	
}
