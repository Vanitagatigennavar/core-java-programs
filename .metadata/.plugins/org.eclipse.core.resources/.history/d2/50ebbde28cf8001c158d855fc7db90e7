package com.xworkz.object_class_method.finalizemethod.biriyani;

public class Biriyani {
	String type;
	double price;
	
	Biriyani(String tyfe,double price){
	this.type=type;
	this.price=price;
	}

	@Override
	public String toString() {
		return "Biriyani [type=" + type + ", price=" + price + "]";
	}
	
	
	

	}

	

   
	
	

