package com.xworkz.core_java.inheritance.multilevel_inheritance.bird;

public class BabyPeacock extends Peacock{
	void follow() {
		System.out.println("follows peacock");
		}
	void play() {
		System.out.println("baby peacock is playing");
		}
	public static void main(String[] args) {
	Bird bird =new Bird();
	Peacock peacock=new Peacock();
	BabyPeacock babypeacock=new BabyPeacock();

	
}
